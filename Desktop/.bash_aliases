### Windows Username for WSL 
export WINUSER=telmi

#------------------------------------------------------------
#  Command alias
#------------------------------------------------------------
alias rm="rm -i"
alias clr="clear"
alias ls="ls --color=auto"
alias la="ls -a"
alias ll="ls -l"
alias l="ls -la"
alias ks="ls"
alias sl="ls"
alias src="source"
alias ps="ps --sort=start_time"
alias htop="htop -us"
alias py3="python3"
alias vi="vim"

#------------------------------------------------------------
#  Alias of ChangeDirectory
#------------------------------------------------------------
alias cdc="cd /mnt/c"
alias cddl="cd /mnt/c/Users/${WINUSER}/Downloads"
alias cdw="cd /mnt/s/working"
alias ..="cd ../"

#------------------------------------------------------------
#  Windows Applications
#------------------------------------------------------------
alias pdfv="/mnt/c/Program\ Files/Tracker\ Software/PDF\ Viewer/PDFXCview.exe"
alias xyzzy="/mnt/s/ProgramFiles/xyzzy/xyzzy.exe"
alias discord="/mnt/c/Users/${WINUSER}/AppData/Local/Discord/app-0.0.301/Discord.exe"
alias calc="calc.exe &"
alias ep="explorer.exe ."
alias powershell="powershell.exe"
alias minecraft='/mnt/s/Game/MineCraft/MinecraftLauncher.exe'
alias gsync='/mnt/c/Program\ Files/Google/Drive/googledrivesync.exe &'

